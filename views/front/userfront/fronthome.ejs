<!DOCTYPE html>
<html>

<%-include('./common/head')%>

  <body>

    <!-- header section strats -->
    <header class="header_section">
      <div class="container-fluid">
        <nav class="navbar navbar-expand-lg custom_nav-container">
          <a class="navbar-brand" href="/USER/user/home">
            <span>
              Healet
            </span>
            <span>
              <h6>
                <%=data.name%>
              </h6>
            </span>
          </a>
          <div class="" id="">

            <div class="custom_menu-btn">
              <button onclick="openNav()">
                <span class="s-1"> </span>
                <span class="s-2"> </span>
                <span class="s-3"> </span>
              </button>
              <div id="myNav" class="overlay">
                <div class="overlay-content">
                  <a href="/USER/user/home">Home</a>
                  <a href="/USER/user/about">About</a>
                  <a href="/USER/user/shop">Shop</a>
                  <a href="/USER/user/blog">Blog</a>
                  <a href="/USER/Dashboard/<%=data._id%>">Dashboard</a>
                  <a href="/user/update/password/<%=data._id%>">Update Password</a>
                  <a href="/user/logout">LogOut</a>
                </div>
              </div>
            </div>

          </div>
        </nav>
      </div>
    </header>
    <!-- end header section -->

    <!-- slider section -->
    <section class="slider_section position-relative">
      <div class="slider_bg_box">
        <%for(iteam of alldata){%>
          <img src="/<%=iteam.image%>" alt="">

      </div>
      <div class="slider_bg"></div>
      <div class="container">
        <div class="col-md-9 col-lg-8">
          <div class="detail-box">
            <h1>
              <%=iteam.BannerTitle%>
            </h1>
            <p>
              <%=iteam.BannerDescription%>
            </p>
            <!-- <div>
              <a href="" class="slider-link">
                Shop Now
              </a>
            </div> -->
          </div>
        </div>


      </div>
      <%}%>
    </section>
    <!-- end slider section -->

    <!-- shop section -->

    <section class="shop_section layout_padding">
      <div class="container">
        <div class="heading_container heading_center">
          <h2>
            Latest Products
          </h2>
        </div>
        <div class="row">
          <% for(pdata of allproduct){%>
            <div class="col-sm-6 col-md-4 col-lg-3">

              <div class="box">
                <a href="">
                  <div class="img-box">
                    <img src="/<%=pdata.images%>" alt="">
                  </div>
                  <div class="detail-box">
                    <h6>
                      <%=pdata.Productname%>
                    </h6>
                    <a href="/USER/user/produt/details/<%= pdata._id%>" class="btn btn-info">Product Details</a>
                  </div>
                  
                  <div class="new">
                    <span>
                      New
                    </span>
                  </div>
                </a>
              </div>
            </div>
            <%}%>
            
              <!--  -->

        </div>
        <div class="btn-box">
          <a href="/USER/user/shop">
            View All Products
          </a>
        </div>
      </div>
    </section>

    <!-- end shop section -->
    <!-- new arrival -->
    <section class="shop_section layout_padding">
      <div class="container">
        <div class="heading_container heading_center">
          <h2>
            New Arrivals
          </h2>
        </div>
        <div class="row">
          <% for(pdata of allproduct){%>
            <%if(pdata.status=='newarrival' ){%>
              <div class="col-sm-6 col-md-4 col-lg-3">

                <div class="box">
                  <a href="">
                    <div class="img-box">
                      <img src="/<%=pdata.images%>" alt="">
                    </div>
                    <div class="detail-box">
                      <h6>
                        <%=pdata.Productname%>
                      </h6>

                    </div>
                    <div class="new">
                      <span>
                        New
                      </span>
                    </div>
                  </a>
                </div>
              </div>
              <%}}%>
               </div>
        <div class="btn-box">
          <a href="/USER/user/shop">
            View All Products
          </a>
        </div>
      </div>
    </section>
    <!-- end new arrival -->

    <!-- about section -->

    <section class="about_section  ">
      <div class="container">
        <div class="row">
          <div class="col-md-6">
            <div class="img-box">
              <img src="/front/images/about-img.jpg" alt="">
            </div>
          </div>
          <div class="col-md-6">
            <div class="detail-box">
              <div class="heading_container">
                <h2>
                  About Us
                </h2>
              </div>
              <p>
                The Brand is known for its timeless craftsmanship, contemporary designs and exceptional product quality. With the roots in Kolkata, Healet have 65 showrooms located across 9 cities of India like - Agartala, Bhubaneswar, Delhi, Noida, Bangalore, Jamshedpur, Mumbai, Ranchi, Patna and have footprints across multiple locations in West Bengal. Further, crossing over geographical boundaries, Healet Jewellers caters to a diverse category of consumers through its Omnichannel approach.
              </p>
              <a href="">
                Read More
              </a>
            </div>
          </div>
        </div>
      </div>
    </section>

    <!-- end about section -->

    <!-- offer section -->
    <section class="offer_section layout_padding">
      
      <div class="container-fluid">
        
        <div class="row">
          <%for(fdata of offerdata){%>
          <div class="col-md-4 px-0">
            <div class="box offer-box1">
              <img src="/<%=fdata.image%>" alt="">
              <div class="detail-box">
                <h2>
                  <%=fdata.offers%>
                </h2>
                <!-- <a href="">
                  Shop Now
                </a> -->
              </div>
            </div>
          </div>
          <%}%>
        </div>
       
      </div>
      
    </section>

    

    <!-- end offer section -->

