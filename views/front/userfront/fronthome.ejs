          </div>

        </div>
      </div>
    </section>

    <!-- end client section -->

    <!-- info section -->
    <%-include('./common/info')%>
      <!-- end info_section -->


      <!-- footer section -->

      <%-include('./common/footer')%>

        <!-- footer section -->


        <!-- jQery -->
        <script src="/front/js/jquery-3.4.1.min.js"></script>
        <!-- bootstrap js -->
        <script src="/front/js/bootstrap.js"></script>
        <!-- custom js -->
        <script src="/front/js/custom.js"></script>

  </body>

</html>