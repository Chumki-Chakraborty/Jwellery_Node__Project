<!DOCTYPE html>
<html>

<%-include('./common/head')%>

<body>

  <!-- header section strats -->
  <%-include('./common/nav')%>
  <!-- end header section -->

  <!-- shop section -->

  <section class="shop_section layout_padding">
    <div class="container">
      
      <div class="heading_container heading_center">
        <h2>
          Latest Products
        </h2>
      </div>
      <div class="row">
        <%for(pdata of allproduct){%>
        <div class="col-sm-6 col-md-4 col-lg-3">
          <div class="box">
            <a href="">
              <div class="img-box">
                <img src="/<%=pdata.images%>" alt="">
              </div>
              <div class="detail-box">
                <h6>
                  <%=pdata.Productname%>
                </h6>
               </div>
               <!-- <a href="" class="btn btn-info">Product Details</a> -->
              <div class="new">
                <span>
                  New
                </span>
              </div>
            </a>
          </div>
        </div>
      <%}%>
        
      
  </section>

  <!-- end shop section -->

  <!-- info section -->
  <%-include('./common/info')%>

  <!-- end info_section -->


  <!-- footer section -->
  <%-include('./common/footer')%>
  <!-- footer section -->


  <!-- jQery -->
  <script src="/front/js/jquery-3.4.1.min.js"></script>
  <!-- bootstrap js -->
  <script src="/front/js/bootstrap.js"></script>
  <!-- custom js -->
  <script src="/front/js/custom.js"></script>

</body>

</html>